@model IEnumerable<dynamic>

<h2>Employee Statistics by Department</h2>

<table class="table">
    <thead>
        <tr>
            <th>Department</th>
            <th>Employee Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.DepartmentName</td>
                <td>@item.EmployeeCount</td>
            </tr>
        }
    </tbody>
</table>
